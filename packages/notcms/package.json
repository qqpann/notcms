{
  "name": "notcms",
  "version": "0.0.13",
  "description": "NotCMS makes it easy to build a CMS for your website.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "notcms": "./dist/cli.cjs"
  },
  "files": ["dist/**/*"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf ./dist/",
    "lint": "eslint ./src/ --fix",
    "format": "biome format --write .",
    "lint-biome": "biome lint --write .",
    "check": "biome check --write .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qqpann/notcms.git"
  },
  "license": "MIT",
  "author": {
    "name": "Qiushi Pan",
    "email": "17402261+qqpann@users.noreply.github.com",
    "url": "https://github.com/qqpann"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "notcms",
    "cms",
    "content management system",
    "notion",
    "markdown",
    "ts",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/qqpann/notcms/issues"
  },
  "homepage": "https://github.com/qqpann/notcms#readme",
  "dependencies": {},
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.22.0",
    "@inquirer/prompts": "^7.0.1",
    "@types/node": "^22.1.0",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dedent": "^1.5.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.0",
    "typescript": "^5.5.4",
    "vitest": "^3.0.9"
  }
}
